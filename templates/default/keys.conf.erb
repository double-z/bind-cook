key "rndc-key" {
  algorithm hmac-md5;
  secret "0m52aN9EromGq+ONsu6tJw==";
};

key "dhcp-key" {
  algorithm hmac-md5;
  secret "L+Jl4+4onU4Wstfi4pdmnQ==";
